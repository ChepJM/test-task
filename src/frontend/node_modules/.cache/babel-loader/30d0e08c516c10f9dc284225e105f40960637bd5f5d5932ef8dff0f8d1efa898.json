{"ast":null,"code":"import { onMounted, reactive } from \"vue\";\nimport axios from \"axios\";\nimport EmplDetailModal from \"@/views/modal/EmplDetailModal.vue\";\nexport default {\n  __name: 'EmployeeManagement',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const model = reactive({\n      data: [],\n      departments: [],\n      professions: [],\n      selectedEmpl: {\n        name: 'Test',\n        description: 'Test description',\n        profession: '',\n        department: ''\n      },\n      detailVisible: false,\n      mode: ''\n    });\n    onMounted(() => {\n      getEmployees();\n      getProfessions();\n      getDepartments();\n    });\n    function openAddModal() {\n      model.mode = 'Add';\n      model.detailVisible = true;\n    }\n    function showDetail(item) {\n      model.selectedEmpl = item;\n      model.mode = 'Edit';\n      model.detailVisible = true;\n    }\n    function deleteEmployee(empl) {\n      axios.delete(`/api/employee?id=${empl.id}`).then(response => {\n        model.data = response.data;\n      });\n    }\n    function getDepartments() {\n      axios.get('/api/department').then(response => {\n        model.departments = response.data;\n      });\n    }\n    function getProfessions() {\n      axios.get('/api/profession').then(response => {\n        model.professions = response.data;\n      });\n    }\n    function getEmployees() {\n      axios.get('/api/employee').then(response => {\n        model.data = response.data;\n      });\n    }\n    function saveChanges(empl) {\n      if (model.mode === 'Add') {\n        addEmployee(empl);\n      } else if (model.mode === 'Edit') {\n        updateEmployee(empl);\n      }\n    }\n    async function addEmployee(empl) {\n      console.log(empl);\n      await axios.post('/api/employee', empl).then(() => {\n        getEmployees();\n        model.detailVisible = false;\n      });\n    }\n    function updateEmployee(empl) {\n      axios.patch('/api/employee', empl).then(() => {\n        getEmployees();\n        model.detailVisible = false;\n      });\n    }\n    const __returned__ = {\n      model,\n      openAddModal,\n      showDetail,\n      deleteEmployee,\n      getDepartments,\n      getProfessions,\n      getEmployees,\n      saveChanges,\n      addEmployee,\n      updateEmployee,\n      onMounted,\n      reactive,\n      get axios() {\n        return axios;\n      },\n      EmplDetailModal\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["onMounted","reactive","axios","EmplDetailModal","model","data","departments","professions","selectedEmpl","name","description","profession","department","detailVisible","mode","getEmployees","getProfessions","getDepartments","openAddModal","showDetail","item","deleteEmployee","empl","delete","id","then","response","get","saveChanges","addEmployee","updateEmployee","console","log","post","patch"],"sources":["C:/Users/Sergei/Downloads/test-task/test-task/src/frontend/src/views/mgmt/EmployeeManagement.vue"],"sourcesContent":["<template>\r\n  <h1>Employees</h1>\r\n  <hr>\r\n  <button @click=\"openAddModal\">\r\n    <i/> Add\r\n  </button>\r\n  <table class=\"empl-tbl\" v-if=\"model.data.length\">\r\n    <tr>\r\n      <th>Id</th>\r\n      <th>Name</th>\r\n      <th>Description</th>\r\n      <th>Profession</th>\r\n      <th>Department</th>\r\n    </tr>\r\n    <tr class=\"data-row\" v-for=\"item in model.data\" :key=\"item.id\" @click=\"showDetail(item)\">\r\n      <td>{{ item.id }}</td>\r\n      <td>{{ item.name }}</td>\r\n      <td>{{ item.description }}</td>\r\n      <td>{{ item.profession.name }}</td>\r\n      <td>{{ item.department.name }}</td>\r\n      <td><span class=\"close\" @click.stop=\"deleteEmployee(item)\">&times;</span></td>\r\n    </tr>\r\n  </table>\r\n  <section class=\"no-data\" v-else>There are no elements.</section>\r\n  <empl-detail-modal\r\n      v-if=\"model.detailVisible\"\r\n      :empl=\"model.selectedEmpl\"\r\n      :mode=\"model.mode\"\r\n      :depts=\"model.departments\"\r\n      :profs=\"model.professions\"\r\n      @onClose=\"model.detailVisible=false\"\r\n      @onSave=\"saveChanges\"\r\n  />\r\n</template>\r\n\r\n<script setup>\r\n\r\nimport {onMounted, reactive} from \"vue\";\r\nimport axios from \"axios\";\r\nimport EmplDetailModal from \"@/views/modal/EmplDetailModal.vue\";\r\n\r\nconst model = reactive({\r\n  data: [],\r\n  departments: [],\r\n  professions: [],\r\n  selectedEmpl: {\r\n    name: 'Test',\r\n    description: 'Test description',\r\n    profession: '',\r\n    department: ''\r\n  },\r\n  detailVisible: false,\r\n  mode: ''\r\n})\r\n\r\nonMounted(() => {\r\n  getEmployees();\r\n  getProfessions();\r\n  getDepartments();\r\n})\r\n\r\nfunction openAddModal() {\r\n  model.mode = 'Add';\r\n  model.detailVisible = true;\r\n}\r\n\r\nfunction showDetail(item) {\r\n  model.selectedEmpl = item;\r\n  model.mode = 'Edit';\r\n  model.detailVisible = true;\r\n}\r\n\r\nfunction deleteEmployee(empl) {\r\n  axios.delete(`/api/employee?id=${empl.id}`).then((response) => {\r\n    model.data = response.data;\r\n  });\r\n}\r\n\r\nfunction getDepartments() {\r\n  axios.get('/api/department').then((response) => {\r\n    model.departments = response.data;\r\n  });\r\n}\r\n\r\nfunction getProfessions() {\r\n  axios.get('/api/profession').then((response) => {\r\n    model.professions = response.data;\r\n  });\r\n}\r\n\r\nfunction getEmployees() {\r\n  axios.get('/api/employee').then((response) => {\r\n    model.data = response.data;\r\n  });\r\n}\r\n\r\nfunction saveChanges(empl) {\r\n  if (model.mode === 'Add') {\r\n    addEmployee(empl);\r\n  } else if (model.mode === 'Edit') {\r\n    updateEmployee(empl);\r\n  }\r\n}\r\n\r\nasync function addEmployee(empl) {\r\n  console.log(empl)\r\n  await axios.post('/api/employee', empl).then(() => {\r\n    getEmployees();\r\n    model.detailVisible = false;\r\n  });\r\n}\r\n\r\nfunction updateEmployee(empl) {\r\n  axios.patch('/api/employee', empl).then(() => {\r\n    getEmployees();\r\n    model.detailVisible = false;\r\n  });\r\n}\r\n\r\n</script>\r\n\r\n<style scoped>\r\n.empl-tbl {\r\n  margin-top: 2em;\r\n}\r\n\r\n.no-data {\r\n  margin-top: 2em;\r\n}\r\n\r\ntr.data-row {\r\n  cursor: pointer;\r\n  background-color: transparent;\r\n}\r\n\r\ntr.data-row:hover {\r\n  background-color: #f1f1f1;\r\n}\r\n</style>\r\n"],"mappings":"AAqCA,SAAQA,SAAS,EAAEC,QAAQ,QAAO,KAAK;AACvC,OAAOC,KAAK,MAAM,OAAO;AACzB,OAAOC,eAAe,MAAM,mCAAmC;;;;;;;IAE/D,MAAMC,KAAK,GAAGH,QAAQ,CAAC;MACrBI,IAAI,EAAE,EAAE;MACRC,WAAW,EAAE,EAAE;MACfC,WAAW,EAAE,EAAE;MACfC,YAAY,EAAE;QACZC,IAAI,EAAE,MAAM;QACZC,WAAW,EAAE,kBAAkB;QAC/BC,UAAU,EAAE,EAAE;QACdC,UAAU,EAAE;MACd,CAAC;MACDC,aAAa,EAAE,KAAK;MACpBC,IAAI,EAAE;IACR,CAAC,CAAC;IAEFd,SAAS,CAAC,MAAM;MACde,YAAY,CAAC,CAAC;MACdC,cAAc,CAAC,CAAC;MAChBC,cAAc,CAAC,CAAC;IAClB,CAAC,CAAC;IAEF,SAASC,YAAYA,CAAA,EAAG;MACtBd,KAAK,CAACU,IAAI,GAAG,KAAK;MAClBV,KAAK,CAACS,aAAa,GAAG,IAAI;IAC5B;IAEA,SAASM,UAAUA,CAACC,IAAI,EAAE;MACxBhB,KAAK,CAACI,YAAY,GAAGY,IAAI;MACzBhB,KAAK,CAACU,IAAI,GAAG,MAAM;MACnBV,KAAK,CAACS,aAAa,GAAG,IAAI;IAC5B;IAEA,SAASQ,cAAcA,CAACC,IAAI,EAAE;MAC5BpB,KAAK,CAACqB,MAAM,CAAE,oBAAmBD,IAAI,CAACE,EAAG,EAAC,CAAC,CAACC,IAAI,CAAEC,QAAQ,IAAK;QAC7DtB,KAAK,CAACC,IAAI,GAAGqB,QAAQ,CAACrB,IAAI;MAC5B,CAAC,CAAC;IACJ;IAEA,SAASY,cAAcA,CAAA,EAAG;MACxBf,KAAK,CAACyB,GAAG,CAAC,iBAAiB,CAAC,CAACF,IAAI,CAAEC,QAAQ,IAAK;QAC9CtB,KAAK,CAACE,WAAW,GAAGoB,QAAQ,CAACrB,IAAI;MACnC,CAAC,CAAC;IACJ;IAEA,SAASW,cAAcA,CAAA,EAAG;MACxBd,KAAK,CAACyB,GAAG,CAAC,iBAAiB,CAAC,CAACF,IAAI,CAAEC,QAAQ,IAAK;QAC9CtB,KAAK,CAACG,WAAW,GAAGmB,QAAQ,CAACrB,IAAI;MACnC,CAAC,CAAC;IACJ;IAEA,SAASU,YAAYA,CAAA,EAAG;MACtBb,KAAK,CAACyB,GAAG,CAAC,eAAe,CAAC,CAACF,IAAI,CAAEC,QAAQ,IAAK;QAC5CtB,KAAK,CAACC,IAAI,GAAGqB,QAAQ,CAACrB,IAAI;MAC5B,CAAC,CAAC;IACJ;IAEA,SAASuB,WAAWA,CAACN,IAAI,EAAE;MACzB,IAAIlB,KAAK,CAACU,IAAI,KAAK,KAAK,EAAE;QACxBe,WAAW,CAACP,IAAI,CAAC;MACnB,CAAC,MAAM,IAAIlB,KAAK,