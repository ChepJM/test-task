{"ast":null,"code":"import { onMounted, reactive } from \"vue\";\nimport axios from \"axios\";\nimport ProfDetailModal from \"@/views/modal/ProfDetailModal.vue\";\nexport default {\n  __name: 'ProfessionManagement',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const model = reactive({\n      data: [],\n      selectedProf: {\n        name: 'Test',\n        description: 'Test description'\n      },\n      detailVisible: false,\n      mode: ''\n    });\n    onMounted(() => {\n      getProfessions();\n    });\n    function openAddModal() {\n      model.mode = 'Add';\n      model.detailVisible = true;\n    }\n    function showDetail(item) {\n      model.selectedProf = item;\n      model.mode = 'Edit';\n      model.detailVisible = true;\n    }\n    function getProfessions() {\n      axios.get('/api/profession').then(response => {\n        model.data = response.data;\n      });\n    }\n    function deleteProfession(prof) {\n      axios.delete(`/api/profession?id=${prof.id}`).then(response => {\n        if (response.data) {\n          getProfessions();\n        }\n      });\n    }\n    function saveChanges(prof) {\n      if (model.mode === 'Add') {\n        addProfession(prof);\n      } else if (model.mode === 'Edit') {\n        updateProfession(prof);\n      }\n    }\n    function addProfession(prof) {\n      axios.post('/api/profession', prof).then(() => {\n        getProfessions();\n        model.detailVisible = false;\n      });\n    }\n    function updateProfession(prof) {\n      axios.patch('/api/profession', prof).then(() => {\n        getProfessions();\n        model.detailVisible = false;\n      });\n    }\n    const __returned__ = {\n      model,\n      openAddModal,\n      showDetail,\n      getProfessions,\n      deleteProfession,\n      saveChanges,\n      addProfession,\n      updateProfession,\n      onMounted,\n      reactive,\n      get axios() {\n        return axios;\n      },\n      ProfDetailModal\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["onMounted","reactive","axios","ProfDetailModal","model","data","selectedProf","name","description","detailVisible","mode","getProfessions","openAddModal","showDetail","item","get","then","response","deleteProfession","prof","delete","id","saveChanges","addProfession","updateProfession","post","patch"],"sources":["C:/Users/Sergei/Downloads/test-task/test-task/src/frontend/src/views/mgmt/ProfessionManagement.vue"],"sourcesContent":["<template>\r\n  <h1>Professions</h1>\r\n  <hr>\r\n  <button @click=\"openAddModal\">\r\n    <i/> Add\r\n  </button>\r\n  <table class=\"prof-tbl\" v-if=\"model.data.length\">\r\n    <tr>\r\n      <th>Id</th>\r\n      <th>Name</th>\r\n      <th>Description</th>\r\n    </tr>\r\n    <tr class=\"data-row\" v-for=\"item in model.data\" :key=\"item.id\" @click=\"showDetail(item)\">\r\n      <td>{{ item.id }}</td>\r\n      <td>{{ item.name }}</td>\r\n      <td>{{ item.description }}</td>\r\n      <td><span class=\"close\" @click.stop=\"deleteProfession(item)\">&times;</span></td>\r\n    </tr>\r\n  </table>\r\n  <section class=\"no-data\" v-else>There are no elements.</section>\r\n  <prof-detail-modal\r\n      v-if=\"model.detailVisible\"\r\n      :prof=\"model.selectedProf\"\r\n      :mode=\"model.mode\"\r\n      @onClose=\"model.detailVisible=false\"\r\n      @onSave=\"saveChanges\"\r\n  />\r\n</template>\r\n\r\n<script setup>\r\n\r\nimport {onMounted, reactive} from \"vue\";\r\nimport axios from \"axios\";\r\nimport ProfDetailModal from \"@/views/modal/ProfDetailModal.vue\";\r\n\r\nconst model = reactive({\r\n  data: [],\r\n  selectedProf: {\r\n    name: 'Test',\r\n    description: 'Test description',\r\n  },\r\n  detailVisible: false,\r\n  mode: ''\r\n})\r\n\r\nonMounted(() => {\r\n  getProfessions();\r\n})\r\n\r\nfunction openAddModal() {\r\n  model.mode = 'Add';\r\n  model.detailVisible = true;\r\n}\r\n\r\nfunction showDetail(item) {\r\n  model.selectedProf = item;\r\n  model.mode = 'Edit';\r\n  model.detailVisible = true;\r\n}\r\n\r\nfunction getProfessions() {\r\n  axios.get('/api/profession').then((response) => {\r\n    model.data = response.data;\r\n  });\r\n}\r\n\r\nfunction deleteProfession(prof) {\r\n  axios.delete(`/api/profession?id=${prof.id}`).then((response) => {\r\n    if (response.data) {\r\n      getProfessions();\r\n    }\r\n  });\r\n}\r\n\r\nfunction saveChanges(prof) {\r\n  if (model.mode === 'Add') {\r\n    addProfession(prof);\r\n  } else if (model.mode === 'Edit') {\r\n    updateProfession(prof);\r\n  }\r\n}\r\n\r\nfunction addProfession(prof) {\r\n  axios.post('/api/profession', prof).then(() => {\r\n    getProfessions();\r\n    model.detailVisible = false;\r\n  });\r\n}\r\n\r\nfunction updateProfession(prof) {\r\n  axios.patch('/api/profession', prof).then(() => {\r\n    getProfessions();\r\n    model.detailVisible = false;\r\n  });\r\n}\r\n\r\n</script>\r\n\r\n<style scoped>\r\n.prof-tbl {\r\n  margin-top: 2em;\r\n}\r\n\r\n.no-data {\r\n  margin-top: 2em;\r\n}\r\n\r\ntr.data-row {\r\n  cursor: pointer;\r\n  background-color: transparent;\r\n}\r\n\r\ntr.data-row:hover {\r\n  background-color: #f1f1f1;\r\n}\r\n</style>\r\n"],"mappings":"AA+BA,SAAQA,SAAS,EAAEC,QAAQ,QAAO,KAAK;AACvC,OAAOC,KAAK,MAAM,OAAO;AACzB,OAAOC,eAAe,MAAM,mCAAmC;;;;;;;IAE/D,MAAMC,KAAK,GAAGH,QAAQ,CAAC;MACrBI,IAAI,EAAE,EAAE;MACRC,YAAY,EAAE;QACZC,IAAI,EAAE,MAAM;QACZC,WAAW,EAAE;MACf,CAAC;MACDC,aAAa,EAAE,KAAK;MACpBC,IAAI,EAAE;IACR,CAAC,CAAC;IAEFV,SAAS,CAAC,MAAM;MACdW,cAAc,CAAC,CAAC;IAClB,CAAC,CAAC;IAEF,SAASC,YAAYA,CAAA,EAAG;MACtBR,KAAK,CAACM,IAAI,GAAG,KAAK;MAClBN,KAAK,CAACK,aAAa,GAAG,IAAI;IAC5B;IAEA,SAASI,UAAUA,CAACC,IAAI,EAAE;MACxBV,KAAK,CAACE,YAAY,GAAGQ,IAAI;MACzBV,KAAK,CAACM,IAAI,GAAG,MAAM;MACnBN,KAAK,CAACK,aAAa,GAAG,IAAI;IAC5B;IAEA,SAASE,cAAcA,CAAA,EAAG;MACxBT,KAAK,CAACa,GAAG,CAAC,iBAAiB,CAAC,CAACC,IAAI,CAAEC,QAAQ,IAAK;QAC9Cb,KAAK,CAACC,IAAI,GAAGY,QAAQ,CAACZ